{
  "gluu_user": {
    "attributes": [
      ["objectClass"],
      ["oxStatus"],
      ["creationDate"],
      ["LOWER(`uid`)"],
      ["del"],
      ["mail"],
      ["inum"],
      ["displayName"],
      ["sn"],
      ["del","exp"]
    ],
    "static" : [
      [["uid"], "objectClass = \"gluuPerson\""],
      [["exp", "oxAuthExpiration"], "del=true"]
    ]
  },

  "gluu_authorization": {
     "attributes": [
        ["oxAuthExpiration"],
        ["del"],
        ["oxAuthClientId"],
        ["oxAuthUserId"]
      ],
    "static" : [
    ]
  },

  "gluu": {
    "attributes": [
      ["objectClass"],
      ["oxScriptType"],
      ["oxAuthExpiration"],
      ["iat"],
      ["oxAuthClientSecretExpiresAt"],
      ["creationDate"],
      ["tknCde"],
      ["inum"],
      ["displayName"],
      ["oxAuthRegistrationAccessToken"],
      ["uid"],
      ["authzCode"],
      ["oxAuthClaimName"],
      ["mail"],
      ["oxId"],
      ["oxTicket"],
      ["del"],
      ["ssnId"],
      ["oxAuthClientId"],
      ["clnId"],
      ["del","exp"],
      ["objectClass","creationDate","oxStatus"]
    ],
    "static" : [
      [["creationDate"], "objectClass = \"gluuPasswordResetRequest\""],
      [["oxScriptType"], "objectClass = \"oxCustomScript\""],
      [["oxApplicationType", "oxMetricType", "oxStartDate", "oxEndDate"], "objectClass = \"oxMetric\""],
      [["exp", "oxAuthExpiration"], "del=true"]

    ]
  },

  "gluu_site": {
    "attributes": [
        ["objectClass"],
        ["inum"]
    ],
    "static" : [
    ]
  },

  "gluu_statistic": {
    "attributes": [
      ["objectClass"],
      ["oxStartDate"],
      ["del"],
      ["oxAuthExpiration"]
    ],
    "static" : [
    ]
  },

  "gluu_cache": {
    "attributes":[
      ["del","exp"]
    ],
    "static" : [
      [["exp", "oxAuthExpiration"], "del=true"],
      [["oxAuthExpiration"], "objectClass = \"cache\""]
    ]
  },

  "gluu_token": {
    "attributes": [
      ["del","exp"]
    ],
    "static" : [
      [["exp", "oxAuthExpiration"], "del=true"],
      [["grtId"], "objectClass = \"token\""]

    ]
  },

  "__common__": {
    "attributes":[
    ],
    "static":[
    ]
  }

}
